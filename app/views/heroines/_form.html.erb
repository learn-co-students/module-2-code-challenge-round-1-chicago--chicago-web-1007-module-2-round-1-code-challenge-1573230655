<%if flash[:errors]%>
  <%flash[:errors].each do |error|%>
    <%=error%>
    <br>
  <%end%>
<%end%>

<%=form_for @heroine do |f|%>
  <%=f.label :name%>
  <%=f.text_field :name%>
  <br>
  <%=f.label :super_name%>
  <%=f.text_field :super_name%>
  <br>
  <%=f.label :power_id, "Power"%>
  <%=f.collection_select :power_id, Power.all, :id, :name%>
  <br>
  <%=f.submit%>
<%end%>