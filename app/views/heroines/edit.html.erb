<h1>Edit a Heroine!</h1>


<%= form_with model: @heroine do |f|%>
<%=f.label :name%>
<%= f.text_field :name%>
<%= f.label :super_name%>
<%= f.text_field :super_name%>
<%= f.collection_select :power_id, Power.all, :id, :name %>
<%=f.submit%>
<% end %>

<%= button_to "delete", @heroine, method: :delete %>
