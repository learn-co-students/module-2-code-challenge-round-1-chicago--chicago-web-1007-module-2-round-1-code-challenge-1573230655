<h2>Heroines</h2>

<%=form_with(url: "/heroines/search", method: "get") do%>
  <%=label_tag(:q, "Search By Superpower:")%>
  <%=text_field_tag(:q)%>
  <%=submit_tag("Search")%>
<%end%>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%=link_to heroine.super_name, heroine %>
    </li>
    <br>
  <% end %>
</ul>
<%=button_to "Create A New Heroine", new_heroine_path, method: :get%>

